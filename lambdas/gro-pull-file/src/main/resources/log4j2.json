{
  "Configuration": {
    "status": "warn",
    "appenders": {
      "Console": {
        "name": "JsonAppender",
        "target": "SYSTEM_OUT",
        "JsonTemplateLayout": {
          "eventTemplateUri": "classpath:LepLambdaLoggingJsonLayout.json"
        }
      }
    },
    "Loggers": {
      "logger": [
        {
          "name": "JsonLogger",
          "level": "info",
          "additivity": false,
          "AppenderRef": {
            "ref": "JsonAppender"
          }
        },
        {
          "name": "net.schmizz.sshj",
          "level": "trace",
          "additivity": false
        }
      ],
      "Root": {
        "level": "info",
        "AppenderRef": {
          "ref": "JsonAppender"
        }
      }
    }
  }
}
