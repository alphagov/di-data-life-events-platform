{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "issuer1",
      "tokenExpiry": 1200,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "len",
          "claims": {
            "sub": "len",
            "scope": "data_receiver/notify",
            "client_id": "len"
          }
        },
        {
          "requestParam": "scope",
          "match": "internal-inbound",
          "claims": {
            "sub": "internal-inbound",
            "scope": "data_receiver/notify",
            "client_id": "internal-inbound"
          }
        },
        {
          "requestParam": "scope",
          "match": "passthru",
          "claims": {
            "sub": "passthru",
            "scope": "data_receiver/notify",
            "client_id": "passthru"
          }
        },
        {
          "requestParam": "scope",
          "match": "dwp",
          "claims": {
            "sub": "dwp-event-receiver",
            "scope": ["data_retriever/read", "events/poll"],
            "client_id": "dwp-event-receiver"
          }
        },
        {
          "requestParam": "scope",
          "match": "hmrc",
          "claims": {
            "sub": "hmrc-client",
            "scope": ["data_retriever/read", "events/poll"],
            "client_id": "hmrc-client"
          }
        },
        {
          "requestParam": "scope",
          "match": "internal-outbound",
          "claims": {
            "sub": "internal-outbound",
            "scope": "data_retriever/read",
            "client_id": "internal-outbound"
          }
        },
        {
          "requestParam": "scope",
          "match": "maintainer",
          "claims": {
            "sub": "maintainer",
            "scope": ["subscriptions/maintain","pubsub/maintain"],
            "client_id": "maintainer"
          }
        },
        {
          "requestParam": "scope",
          "match": "everything",
          "claims": {
            "sub": "everything",
            "scope": ["subscriptions/maintain", "pubsub/maintain", "data_retriever/read", "events/poll", "data_receiver/notify"],
            "client_id": "everything"
          }
        }
      ]
    }
  ]
}
